AddCommand("secret xray command", {"T3V3031H0Y0", "19072021"}, "secret cmd ez lmao", function()
    if game.Players.LocalPlayer.Character.Head then
   game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Secret Command";
    Text = "x-ray view!";  
    Duration = 5 
	})
for _,v in pairs(workspace:GetDescendants()) do
if v.ClassName == "Part"
or v.ClassName == "SpawnLocation"
or v.ClassName == "WedgePart"
or v.ClassName == "Terrain"
or v.ClassName == "MeshPart" then
v.Material = "Metal"
v.Transparency = 0.7
end
end

for _,v in pairs(workspace:GetDescendants()) do
if v.ClassName == "Decal"
or v.ClassName == "Texture" then
v:Destroy()
end
end

    end

end)


