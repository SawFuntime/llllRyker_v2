AddCommand("oldsky", {"oldsky", "sky2"}, "cry abt it", function()
    if game.Players.LocalPlayer.Character.Head then
  game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Sky changed";
    Text = "reminds me of old ts";  
    Duration = 0.5 
	})
   game:GetService("RunService").RenderStepped:Connect(function()
local LService = game:GetService("Lighting")
LService.ClockTime = 14
end)

for i, v in pairs(game:GetService("Lighting"):GetDescendants()) do
    if v.Parent == game:GetService("Lighting") then
        v:Destroy()
    end
end
    end

end)

AddCommand("smogmode", {"smogmode", "smog"}, "stfu ur unknow", function()
    if game.Players.LocalPlayer.Character.Head then
   game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Smogmode";
    Text = "On";  
    Duration = 0.5 
	})
		local lighting = game.Lighting
	local SunRay = Instance.new("SunRaysEffect",lighting)
 
	local skybox = Instance.new("Sky",lighting)
	skybox.SkyboxBk = "rbxassetid://600830446"
	skybox.SkyboxDn = "rbxassetid://600831635"
	skybox.SkyboxFt = "rbxassetid://600832720"
	skybox.SkyboxLf = "rbxassetid://600886090"
	skybox.SkyboxRt = "rbxassetid://600833862"
	skybox.SkyboxUp = "rbxassetid://600835177"
 
	while true do
	wait(0.05)
	local ColorC = game.Lighting.ColorCorrection
	ColorC.TintColor = Color3.fromRGB(26, 250, 252)
	end
end
end)

